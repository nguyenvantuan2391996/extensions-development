{
  "name": "Bubu Dudu GIF Picker Extension",
  "description": "Bubu Dudu GIF Picker Extension is a simple and cute Chrome extension that lets you browse, add, and select your favorite GIFs with just one click.",
  "version": "1.0.0",
  "action": {
    "default_popup": "src/popup.html",
    "default_icon": "images/bubu-dudu.png"
  },
  "icons": {
    "128": "images/bubu-dudu.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/content.js",
        "js/constants.js",
        "js/utils.js"
      ],
      "run_at": "document_start"
    }
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "host_permissions": [
    "*://*/*"
  ],
  "permissions": [
    "webNavigation",
    "storage"
  ],
  "manifest_version": 3
}