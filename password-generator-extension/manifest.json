{
  "name": "Password Generator Extension",
  "description": "The extension supports generating random password strongest.",
  "version": "1.0.0",
  "action": {
    "default_popup": "src/popup.html",
    "default_icon": "images/detector-apis-extension.png"
  },
  "icons": {
    "128": "images/detector-apis-extension.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/constants.js",
        "js/utils.js"
      ],
      "run_at": "document_start"
    }
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "host_permissions": [
    "*://*/*"
  ],
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "webRequest"
  ],
  "manifest_version": 3
}